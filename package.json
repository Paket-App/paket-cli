{
  "name": "paket-cli",
  "description": "CLI para construir proyectos con paket :)",
  "version": "0.0.0",
  "author": "Tuysh",
  "bin": {
    "paket": "./bin/run.js"
  },
  "bugs": "https://github.com/Paket-App/paket-cli/issues",
  "dependencies": {
    "@inquirer/prompts": "^7.3.1",
    "@oclif/core": "^4.2.6",
    "@oclif/plugin-help": "^6.2.25",
    "@oclif/plugin-plugins": "^5.4.31",
    "cheerio": "^1.0.0",
    "chokidar": "^4.0.3",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.4.1",
    "rollup-plugin-import-css": "^3.5.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.1.9",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.15",
    "@types/chai": "^4.3.20",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.75",
    "chai": "^4.5.0",
    "eslint": "^9.20.1",
    "eslint-config-oclif": "^6.0.10",
    "eslint-config-prettier": "^10.0.1",
    "mocha": "^10.8.2",
    "oclif": "^4.17.27",
    "rolldown": "1.0.0-beta.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/Paket-App/paket-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "paket",
    "dirname": "paket",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "Paket-App/paket-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
